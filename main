#!/bin/bash


convert $1 -colorspace Gray -threshold 35% output_image.png
tesseract output_image.png questions/ocr -l deu


inputFile='questions/ocr.txt'
text=$(cat $inputFile)

llama-cli -m /var/models/mistral-7b-instruct-v0.2.Q8_0.gguf \
--in-prefix "solve the test" \
-p "$text\n\nAnswers:\n\n" -no-cnv
